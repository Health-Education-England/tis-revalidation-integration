{
  "containerDefinitions": [
    {
      "name": "tis-revalidation-integration",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/tis-revalidation-integration:1",
      "portMappings": [
        {
          "containerPort": 8088
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "tis-revalidation-integration"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-tis-revalidation-integration",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "awslogs-tis-revalidation-integration"
        },
        "secrets": [
          {
            "name": "CONCERN_HOST",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-preprod-lb-url"
          },
          {
            "name": "CONCERN_PORT",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-concern-port"
          },
          {
            "name": "GMC_CLIENT_HOST",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-preprod-lb-url"
          },
          {
            "name": "GMC_CLIENT_PORT",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-gmc-client-port"
          },
          {
            "name": "RECOMMENDATION_HOST",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-preprod-lb-url"
          },
          {
            "name": "RECOMMENDATION_PORT",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-recommendation-port"
          },
          {
            "name": "TCS_HOST",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-preprod-lb-url"
          },
          {
            "name": "TCS_PORT",
            "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/tis-revalidation-tcs-port"
          }
        ]
      }
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "tis-revalidation-integration",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
